- var title = "Get Started"
include partials/_header

<section class="main-container">
    <div class="offer-form-container ">
        <span class="online-cash">Complete Our Online Form to Get a Loan</span>
        <span class="get-started">Get fast decision and get your loan in as soon as one business day if approved
        </span>
        <div class="offer-form formheader">

            <script src="files/loanRequestForm.js"></script>

        </div>
        <div class="offer-form-features">
            <div class="features-wrap">
                <div class="features-item">
                    <div class="feature-title f1">Request Online</div>
                    <div class="feature-text">There are no fees to submit your request and no obligation to accept the loan</div>
                </div>
                <div class="features-item">
                    <div class="feature-title f2">Safe and Secure</div>
                    <div class="feature-text">You can be sure that your information remains safe and secure with us </div>
                </div>
                <div class="features-item">
                    <div class="feature-title f3">Get Results Fast</div>
                    <div class="feature-text">After you submit your form you can be matched with one of our lenders </div>
                </div>
                <div class="features-item">
                    <div class="feature-title f4">Cash Straight Away</div>
                    <div class="feature-text">If you agree to the lender's terms, e-sign the contract and have cash deposited into your account </div>
                </div>
            </div>
        </div>
    </div>

</section>

include partials/_footer
